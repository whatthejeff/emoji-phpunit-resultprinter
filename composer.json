    "keywords": ["emoji","phpunit","tests"],
    "homepage": "http://github.com/whatthejeff/emoji-phpunit-resultprinter",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Welch",
            "email": "whatthejeff@gmail.com"
        }
